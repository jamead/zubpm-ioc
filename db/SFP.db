record(ai, "$(P){BPM:$(B)}SFP${S}:Temp-I") {
    field(DESC, "SFP${S} Temperature (C)")
    field(DTYP, "PSC Reg F32")
    field(INP , "@Rx$(B) 32 $(TMP)")
    field(LINR,"LINEAR")
    field(PREC,"3")
    field(SCAN,"I/O Intr")
    field(EGU,  "C")
    field(HIHI,"60")
    field(LOLO,"1")
    field(HIGH,"50")
    field(LOW,"10")
    field(HHSV,"MAJOR")
    field(HSV,"MINOR")
    field(LLSV,"INVALID")
    field(LSV,"MINOR")
}

