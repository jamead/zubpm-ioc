record(stringin, "$(P){BPM:$(NO)}FA:Hdr:Buff-Wfm:T") {
  field(DTYP, "Soft Timestamp")
  field(TSEL, "$(P){BPM:$(NO)}FA:Hdr:Buff-Wfm.TIME CP")
  field(INP, "@%Y%H%M%S")
}

